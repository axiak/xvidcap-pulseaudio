# type: Content of: <refentry><refsect1><variablelist><varlistentry><listitem><para><literal>
#: xvidcap.1.compact.xml:1 xvidcap.1.compact.xml:1 xvidcap.1.compact.xml:1 xvidcap.1.compact.xml:1
#, no-wrap
msgid "0"
msgstr ""

# type: Content of: <refentry><refsect1><variablelist><varlistentry><listitem><para>
#: xvidcap.1.compact.xml:1 xvidcap.1.compact.xml:1
#, no-wrap
msgid "and"
msgstr ""

# type: Content of: <refentry><refsect1><variablelist><varlistentry><listitem><para><literal>
#: xvidcap.1.compact.xml:1
#, no-wrap
msgid "100"
msgstr ""

# type: Content of: <refentry><refsect1><variablelist><varlistentry><listitem><para>
#: xvidcap.1.compact.xml:1
#, no-wrap
msgid "to define the quality encoding. The default value is"
